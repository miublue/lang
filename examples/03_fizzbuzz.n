extern fun putstr(s: str), fun putnum(n: int);

fun fizzbuzz(n: int) {
    i: int = 1;
    while (i <= n) {
        if (i % 15 == 0) putstr("FizzBuzz\n");
        else if (i % 3 == 0) putstr("Fizz\n");
        else if (i % 5 == 0) putstr("Buzz\n");
        else putnum(i);
        i = i + 1;
    }
}

fun main() fizzbuzz(100);

